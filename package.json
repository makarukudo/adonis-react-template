{
  "name": "adonis-fullstack-app",
  "version": "4.1.0",
  "adonis-version": "4.1.0",
  "description": "The fullstack application boilerplate for Adonisjs",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently --kill-others-on-fail \"adonis serve --dev\" \"npm run development\"",
    "start": "cross-env NODE_ENV=production node server.js",
    "test": "node ace test",
    "deploy": "npx shipit staging deploy && npx shipit staging restart",
    "development": "cross-env NODE_ENV=development node_modules/.bin/webpack --watch --hide-modules --mode development"
  },
  "keywords": [
    "adonisjs",
    "adonis-app"
  ],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
